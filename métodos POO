package metodos;

public class Metodos {
	
	public  void somaDoisNumeros() {//metodo sem retorno 
		System.out.println("A soma é: " + (10 + 10));
		
	}
	public void subDoisNumeros() {
		System.out.println("A subtração é: " + (20 - 10));
	}
		 
	public void multiplica(int num1, int num2) {
		System.out.println("A multiplicação é: " + num1 * num2);
	}
	
	public double divide(double num1, double num2) {
		return num1/num2;
	}
	}

